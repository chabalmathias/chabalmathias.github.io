@incollection{brazier_210_2013,
	address = {San Diego},
	title = {2.10 {Hillslope} {Soil} {Erosion} {Modeling}},
	isbn = {978-0-08-088522-3},
	abstract = {Environmental models are simplifications of the real world, which seek to represent highly complex systems, in order to predict likely outcomes, Approaches have been developed to predict soil erosion by water, at the hillslope scale. A number of conceptual approaches are discussed, from the empirically based algorithm of the 1960s, the Universal Soil Loss Equation, through to its process-based successor the Water Erosion Prediction Project, alongside a range of other models that have been developed over the last five decades. Scales of model application are discussed, illustrating the range of space and time that models are applied across and the different problems that are associated with overcoming the scaling problem in soil erosion prediction. The evaluation of soil erosion models is detailed, first in terms of model sensitivity analyses and then in terms of model uncertainty analyses. Finally, the chapter focuses on recent models, which have been developed as research tools, to explore understanding of the physics of erosion. One such model, Model for the Assessment of Hillslope to Landscape Erosion, Runoff and Nutrients (MAHLERAN), is proposed as an approach that may be followed to improve both process representation in soil erosion models and the way in which erosion models are developed, in dialog with field data collection and laboratory experiments.},
	booktitle = {in {Treatise} on {Geomorphology}},
	publisher = {Academic Press},
	author = {Brazier, R. E.},
	editor = {Shroder, John F.},
	month = jan,
	year = {2013},
	keywords = {Hillslope, Hydrology, Modeling, Scale, Sensitivity analysis, Soil erosion, Uncertainty},
	pages = {135--146},
}

