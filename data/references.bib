	keywords = {Avulsion, Channel, Channel bar, Channel fill, Crevasse splay, Flood basin, Floodplain, Fluvial, Holocene, Mississippi River, Natural levee, Quaternary, River, Sediments, Terrace},
	pages = {663--675},
}

